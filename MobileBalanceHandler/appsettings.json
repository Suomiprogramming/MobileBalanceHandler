{
  "ConnectionStrings": {
    "Connection": "DataSource = mobileBalance.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Carrier": [
    {
      "Name": "Activ",
      "Prefix": [
        "701",
        "702",
        "778",
        "775"
      ],
      "RequestType": "SOAP",
      "Url": "http://localhost:5003/Activ/SendMoney"
    },
    {
      "Name": "Beeline",
      "Prefix": [
        "705",
        "776",
        "771",
        "777"
      ],
      "RequestType": "HTTP",
      "Url": "http://localhost:5004/Beeline/AddBalance"
    },
    {
      "Name": "Tele2",
      "Prefix": [
        "707",
        "747"
      ],
      "RequestType": "SOAP",
      "Url": "http://localhost:5006/Tele2/AddCash"
    },
    {
      "Name": "Altel",
      "Prefix": [
        "708",
        "700"
      ],
      "RequestType": "HTTP",
      "Url": "http://localhost:5002/Altel/AddFunds"
    }
  ]
}
